@model Garage2._0.Models.Statistic

@{
    ViewData["Title"] = "GetStatistic";
}
    <div class=" border border-dark border rounded  align-content-center justify-content-center p-3 w-75">
        <h1 class="bg-primary border rounded-lg font-italic text-light p-2">Parking Statistic</h1>
        


        <div>

            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TotalCar)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TotalCar)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TotalBoat)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TotalBoat)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TotalBus)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TotalBus)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TotalAirplane)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TotalAirplane)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TotalMotorbike)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TotalMotorbike)
                </dd>



            </dl>



        </div>
      
        <div>

            <hr />
            <dl class="row">
                <dt class="col-sm-6 h5">
                    <p class="text-info h5 m-2">No. of Free Places:</p>

                </dt>
                <dd class="col-sm-6 h5">
                    @ViewBag.NoOfFreePlaces
                </dd>
                <dt class="col-sm-6 h5">
                    <p class="text-info h5 m-2">No. of Free Places for Motorcycle:</p>
                </dt>
                <dd class="col-sm-6 h5">
                    @ViewBag.NoOfFreePlacesForMotorcycle
                   
                </dd>
                <dt class="col-sm-6 h5">
                    <p class="text-info h5 m-2">Total Vehicles :</p>
                </dt>
                <dd class="col-sm-6 h5">
                    @Html.DisplayFor(model => model.TotalVehicles)  Vehicles
                </dd>
                <dt class="col-sm-6 h5">
                    <p class="text-info h5 m-2">Total Wheels in the parking :</p>
                </dt>
                <dd class="col-sm-6 h5">
                    @Html.DisplayFor(model => model.TotalWheels) Wheels
                </dd>
                <dt class="col-sm-6">
                    <p class="text-info h5 m-2"> Total Parked Vehicle Price :</p>
                </dt>
                <dd class="col-sm-6 h5">
                    @Html.DisplayFor(model => model.TotalParkedVehiclePrice) kr
                </dd>
            </dl>
        </div>
    </div>
<div>
    @*Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |*@
    <a class="btn btn-outline-primary mt-3"  asp-action="Index">Back to List</a>
</div>

@model Garage2._0.Models.Receipt

@{
    ViewData["Title"] = "Receipt";
}



<div id="print" class=" border border-dark p-5 align-content-center justify-content-center">
    <h1 class="text-center"><u>Receipt</u></h1>
    <br />
    <dl class="row ">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.RegNo)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.RegNo)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.CheckInTime)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.CheckInTime)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.CheckOutTime)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.CheckOutTime)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.Totalparkingtime)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Totalparkingtime)
        </dd>
        <dt class="col-sm-3 text-right">
            @Html.DisplayNameFor(model => model.Totalprice)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.Totalprice)
        </dd>
    </dl>
</div>
<div class="mb-5 align-content-center">
    <input class="btn-primary btn-lg" type="button" value="Print" onclick="window.print();">
</div>
